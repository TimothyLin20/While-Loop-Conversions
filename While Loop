# 1
str = "hello"
i=0
while i < len(str):     
    print(str[i])
    i+=1

# 2
numbers = [7, 7, 2, 7, 11]
i=0
while i<len(numbers):
    print(numbers[i])
    i+=1

# 3
marks = [84, 89, 90, 45, 67]
i=0
while i<len(marks):
    print(marks[i])
    i+=1

# 4
i=0
while i<10:
    print(i)
    i+=1

# 5
i=5
while i<100:
    print(i)
    i+=5

# 6
i=100
while i>-1:
    print(i)
    i-=5
